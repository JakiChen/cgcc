			},
			"inLanguage": "en-us",
			"mainEntityOfPage": "{{ site.url }}/privacy-policy",
			"name": "Privacy And Cookie Policy",
			"publisher": {
				"@id": "{{ site.url }}#creator"
			},
			"url": "{{ site.url }}/privacy-policy"
		}, {
			"@id": "{{ site.url }}#identity",
			"@type": "LocalBusiness",
			"description": "{{ PAGE_DESCRIPTION }}",
			"email": "info@{{ site.url }}",
			"image": {
				"@type": "ImageObject",
				"height": "3750",
				"url": "{{ site.url }}/uploads/Chapiter_branding_agency_reel.jpg",
				"width": "6667"
			},
			"logo": {
				"@type": "ImageObject",
				"height": "60",
				"url": "{{ site.url }}/uploads/_600x60_fit_center-center_82_none/177908/Chapiter_branding_agency_reel.png?mtime=1578417383",
				"width": "107"
			},
			"name": "Chapiter",
			"priceRange": "$",
			"sameAs": [{{ site.social.links | join: "," }}],
			"url": "{{ site.url }}"
		}, {
			"@id": "#creator",
			"@type": "Organization"
		}, {
			"@type": "BreadcrumbList",
			"description": "Breadcrumbs list",
			"itemListElement": [{
				"@type": "ListItem",
				"item": "{{ site.url }}/",
				"name": "Home",
				"position": 1
			}, {
				"@type": "ListItem",
				"item": "{{ site.url }}/privacy-policy",
				"name": "Privacy And Cookie Policy",
				"position": 2
			}],
			"name": "Breadcrumbs"
		}]
	}
</script>